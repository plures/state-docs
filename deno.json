
{
  "name": "@plures/statedoc",
  "version": "1.0.2",
  "exports": {
    ".": "./mod.ts",
    "./cli": "./cli.ts"
  },
  "license": "MIT",
  "tasks": {
    "gen": "deno run -A cli.ts --config=.stateDoc.json",
    "watch": "deno run -A --watch=src,fsm cli.ts --config=.stateDoc.json",
    "build:npm": "deno run -A scripts/build_npm.ts"
  },
  "lint": { "rules": { "exclude": ["no-explicit-any"] } },
  "imports": {
    "@deno/dnt": "jsr:@deno/dnt@^0.42.3",
    "@std/fs": "jsr:@std/fs@^1",
    "@std/path": "jsr:@std/path@^1",
    "handlebars": "npm:handlebars@^4.7.8"
  }
}
